<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 
	<servlet>
		<servlet-name>CalculatorPojoWsRpc</servlet-name>
		<servlet-class>
			com.ivankoi.jee6.business.calculator.boundary.CalculatorPojoWsRpc
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CalculatorPojoWsDoc</servlet-name>
		<servlet-class>
			com.ivankoi.jee6.business.calculator.boundary.CalculatorPojoWsDoc
		</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>CalculatorPojoWsRpcSecured</servlet-name>
		<servlet-class>
			com.ivankoi.jee6.business.calculator.boundary.CalculatorPojoWsRpcSecured
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CalculatorPojoWsDocSecured</servlet-name>
		<servlet-class>
			com.ivankoi.jee6.business.calculator.boundary.CalculatorPojoWsDocSecured
		</servlet-class>
	</servlet>
  
  <servlet>
    <servlet-name>Endpoint</servlet-name>
    <servlet-class>org.jboss.test.ws.jaxws.samples.provider.ProviderBeanPayload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Endpoint</servlet-name>
    <url-pattern>/Endpoint</url-pattern>
  </servlet-mapping>
  
	<servlet-mapping>
		<servlet-name>CalculatorPojoWsRpc</servlet-name>
		<url-pattern>/CalculatorPojoWsRpc</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CalculatorPojoWsDoc</servlet-name>
		<url-pattern>/CalculatorPojoWsDoc</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CalculatorPojoWsRpcSecured</servlet-name>
		<url-pattern>/CalculatorPojoWsRpcSecured</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CalculatorPojoWsDocSecured</servlet-name>
		<url-pattern>/CalculatorPojoWsDocSecured</url-pattern>
	</servlet-mapping>

 </web-app>
